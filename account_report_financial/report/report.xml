<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="report_financial_paperformat" model="report.paperformat">
            <field name="name">Report Financial</field>
            <field name="default" eval="True"/>
            <field name="orientation">Landscape</field>
        </record>

        <report
            id="action_report_financial_template"
            model="wizard.report.financial"
            string="Report Financial"
            report_type="qweb-pdf"
            name="account_report_financial.report_financial"
            file="account_financial_report.report_financial"
            menu="False"
            paperformat="report_financial_paperformat"
        />

    </data>
</odoo>