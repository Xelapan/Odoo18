        <record id="wizard_report_financial_form_view" model="ir.ui.view">
            <field name="name">wizard.report.financial.form.view</field>
            <field name="model">wizard.report.financial</field>
            <field name="arch" type="xml">
                <form>
                    <field name="state" invisible="1" />
                    <field name="name" invisible="1" />
                    <group modifiers="{}">
                        <field name="company_id" required="1" groups="base.group_multi_company" options="{'no_open': True, 'no_create': True, 'no_create_edit': True}" readonly="1" />
                    </group>
                    <group col="4" modifiers="{}">
                        <field name="anio" required="1" />
                        <field name="mes_de" required="1" />
                        <field name="mes_a" required="1" />
                        <field name="folio" required="1" />
                    </group>

                    <div modifiers="{}">
                            <h2>Descargar Archivo - Libro Diario</h2>
                            <p>File : <field name="data" readonly="1" filename="name" /></p>
                    </div>
                    <footer modifiers="{}">
                        <button name="print_xls_report_financial" string="Libro Diario Excel" type="object" class="oe_highlight" />
                        or
                        <button string="Cancelar" class="oe_link" special="cancel" />
                    </footer>
                    <footer modifiers="{}">
                        <button name="go_back" string="Atras" type="object" class="oe_highlight" />
                        or
                        <button special="cancel" string="Cerrar" type="object" class="btn-primary" />
                    </footer>
                </form>
            </field>
        </record>
