<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!--Vista heredada de stock_account.stock_valuation_layer_tree-->
    <record id="stock_valuation_layer_tree_inherit" model="ir.ui.view">
        <field name="name">stock.valuation.layer.tree.inherit</field>
        <field name="model">stock.valuation.layer</field>
        <field name="inherit_id" ref="stock_account.stock_valuation_layer_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_id']" position="after">
                <field name="category_id"/>
<!--                <t t-call-assets="bi_inventory_valuation_reports.assets_backend_inherit"/>-->
            </xpath>
            <xpath expr="//field[@name='reference']" position="after">
                <field name="location_id"/>
                <field name="location_dest_id"/>
<!--                <t t-call-assets="bi_inventory_valuation_reports.assets_backend_inherit"/>-->
            </xpath>

        </field>
    </record>
</odoo>
