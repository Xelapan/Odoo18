<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_account_asset_viaceq_wizard_form" model="ir.ui.view">
            <field name="name">account.assett.viaceq.wizard.form</field>
            <field name="model">account.asset.viaceq.wizard</field>
            <field name="arch" type="xml">
                <form string="Reporte de Equipo Fungible">
                    <group>
                        <field name="date_from"/>
                        <field name="date_to"/>
                        <field name="account_ids" widget="many2many_tags"/>
                        <field name="company_id" readonly="1"/>

                    </group>
                    <footer>
                        <button name="open_account_asset_viaceq_at_date" type="object" string="Reporte Activos" class="btn-primary"/>
                        <button string="Cancelar" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_account_asset_viaceq_wizard" model="ir.actions.act_window">
            <field name="name">Consultar Reporte de Equipo Fungible</field>
            <field name="res_model">account.asset.viaceq.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="view_id" ref="view_account_asset_viaceq_wizard_form"/>
        </record>
    </data>
</odoo>