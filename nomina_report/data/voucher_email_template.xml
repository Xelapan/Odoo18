<odoo>
    <data noupdate="1">
        <record id="email_template_voucher" model="mail.template">
            <field name="name">Salary Voucher Email</field>
            <field name="model_id" ref="hr_payroll.model_hr_payslip"/>
            <field name="auto_delete" eval="False"/>
            <field name="subject">Recibo de pago</field>
            <field name="email_from">talentohumanosgestion@gmail.com</field>
            <field name="email_to">{{object.contract_id.employee_id.work_email}}</field>
            <field name="body_html">
                <![CDATA[
                <div>
                    <p>Estimado(a) <t t-out="object.employee_id.name"/>,</p>
                    <p>Adjunto comprobante de compensación correspondiente al período.</p>
                </div>
                   ]]>
            </field>
            <field name="report_template" ref="report_voucher"/>
            <field name="report_name">Recibo de Pago {{object.employee_id.name}}</field>
        </record>
    </data>
</odoo>