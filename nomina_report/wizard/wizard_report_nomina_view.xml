        <record id="wizard_report_facturacion_form_view" model="ir.ui.view">
            <field name="name">wizard.report.facturacion.form.view</field>
            <field name="model">wizard.report.facturacion</field>
            <field name="arch" type="xml">
                <form string="Reporte de Facturación">
                    <field name="state" invisible="1" />
                    <field name="name" invisible="1" />
                    <group col="4" modifiers="{}">
                        <field name="planillas" required="1" widget="many2many_tags" />--&gt;


                    </group>
                    <div modifiers="{}">
                        <h2> Descargar Archivo - Reporte de Facturación</h2>
                        <p>File: <field name="data" readonly="1" filename="name" /> </p>
                    </div>
                    <footer modifiers="{}">
                        <button name="print_xls_reporte_facturacion" string="Reporte de Facturación" type="object" class="oe_highlight" />
                        or
                        <button string="Cancelar" class="oe_link" special="cancel" />
                    </footer>
                    <footer modifiers="{}">
                        <button name="got_back" string="Atras" type="object" class="oe_highlight" />
                        or
                        <button special="cancel" string="Cerrar" type="object" class="btn-primary" />
                    </footer>
                </form>
            </field>
        </record>

